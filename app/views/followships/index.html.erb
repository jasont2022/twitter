<p id="notice"><%= notice %></p>

<h1>Followships</h1>

<table>
  <thead>
    <tr>
      <th>Follower</th>
      <th>Following</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @followships.each do |followship| %>
      <tr>
        <td><%= followship.follower_id %></td>
        <td><%= followship.following_id %></td>
        <td><%= link_to 'Show', followship %></td>
        <td><%= link_to 'Edit', edit_followship_path(followship) %></td>
        <td><%= link_to 'Destroy', followship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Followship', new_followship_path %>
